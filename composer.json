{
    "name": "mediaopt/dhl-oxid",
    "type": "oxideshop-module",
    "keywords": ["oxid", "modules", "eShop"],
    "license": "proprietary",
    "config": {
        "prepend-autoloader": false,
        "vendor-dir": "src/modules/mo/mo_dhl/vendor"
    },
    "require": {
        "php": ">=7.0",
        "monolog/monolog": "1.12.*",
        "guzzlehttp/guzzle": "^6.0",
        "roave/security-advisories": "dev-master",
        "ext-json": "*",
        "ext-calendar": "*",
        "ext-iconv": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5",
        "mjaschen/phpgeo": "^1.3",
        "fzaninotto/faker": "^1.7",
        "guzzlehttp/streams": "^3.0"
    },
    "extra": {
        "oxideshop": {
            "source-directory": "src/modules/mo/mo_dhl",
            "target-directory": "mo/mo_dhl"
        }
    },
    "autoload": {
        "psr-4": {
            "Mediaopt\\DHL\\": "src/modules/mo/mo_dhl"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@bitbucket.org:mediaopt/wunschpaket-sdk.git"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
